{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
	"indent": [true, "tabs", 4],
	"trailing-comma": false,
    "no-console": false,
	"object-literal-sort-keys": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
